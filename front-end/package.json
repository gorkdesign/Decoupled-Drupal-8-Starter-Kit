{
  "name": "d8d-front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.0.3",
    "apollo-client-preset": "^1.0.5",
    "apollo-link": "^1.1.0",
    "apollo-link-http": "^1.3.3",
    "axios": "^0.17.1",
    "dropzone": "^5.2.0",
    "extract-files": "^3.0.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.6.1",
    "history": "^4.7.2",
    "object-path": "^0.11.4",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.5",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-transition-group": "^2.2.1",
    "reactstrap": "5.0.0-beta",
    "recursive-iterator": "^3.3.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/react": "^16.0.5",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "bootstrap": "^4.0.0",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "react-scripts": "1.0.17",
    "react-test-renderer": "^16.2.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "react-addons-test-utils": "^15.6.2",
    "selenium-standalone": "^6.12.0",
    "wdio-dot-reporter": "0.0.8",
    "wdio-mocha-framework": "^0.5.12",
    "webdriverio": "^4.10.2"
  },
  "scripts": {
    "selenium-setup": "selenium-standalone install",
    "selenium-start": "selenium-standalone start",
    "e2e-tests": "wdio wdio.conf.js",
    "e2e-tests-watch": "wdio wdio.conf.js --watch",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "NODE_PATH=src react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar --output-style nested --source-map-embed true --source-map true --source-map-root ./src/styles --include-path ./src/styles src/styles/ -o ./src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --output-style nested --source-map-embed true --source-map true --source-map-root ./src/styles --include-path ./src/styles src/styles/ -o ./src/styles/ --watch --recursive",
    "sass": "sass --watch src/styles/scss:src/styles/css",
    "test": "NODE_PATH=src react-scripts test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "eject": "react-scripts eject"
  }
}
